{
    "name": "div-art/shortlink",
    "description": "A Laravel 5.5 shortener package",
    "require": {
        "php": ">=7.0",
        "guzzlehttp/guzzle": "6.*",
        "illuminate/support": "~5"
    },
    "license": "MIT",
    "keywords": ["shortlink", "div-art", "laravel"],
    "authors": [
        {
            "name": "Div-Art",
            "email": "info@div-art.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "DivArt\\ShortLink\\": "src/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "DivArt\\ShortLink\\ShortLinkServiceProvider"
            ],
            "aliases": {
                "ShortLink": "DivArt\\ShortLink\\Facades\\ShortLink"
            }
        }
    },
    "repositories": {
        "packagist": { "url": "https://packagist.org", "type": "composer" }
    }
}
